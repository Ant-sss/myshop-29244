<h2>住所登録<h2>


<%= form_for @residence do |f| %>
  <%= render "devise/shared/error_messages", resource: @residence %>

  <div class="field">
    <%= f.label :郵便番号 %><br />
    <%= f.text_field :postal_code %>
  </div>

    <div class="field">
    <%= f.label :都道府県 %><br />
    <%= f.collection_select(:prefecture_id, Prefecture.all, :id, :name, {}, {class:"prefecture_select"}) %>
  </div>

  <div class="field">
    <%= f.label :市町村区 %><br />
    <%= f.text_field :city %>
  </div>

    <div class="field">
    <%= f.label :番地 %><br />
    <%= f.text_field :house_num %>
  </div>

    <div class="field">
    <%= f.label :建物名 %><br />
    <%= f.text_field :building_name %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>